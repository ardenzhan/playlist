<p><a href='<%= songs_path %>'>Dashboard</a></p>
<h1>People who added <%= @song.artist %> - <%= @song.title %></h1>
<table>
    <thead>
        <tr>
            <td>Name</td>
            <td>Times Added</td>
        </tr>
    </thead>
    <tbody>
        <% @addusers.each do |user| %>
            <tr>
                <td><a href="<%= user_path user.id %>"><%= user.first_name %> <%= user.last_name %></a></td>
                <td><%= @addcount[user.id] %></td>
            </tr>
        <% end %>
    </tbody>
</table>